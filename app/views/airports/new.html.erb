<h1>New Airport</h1>


<%= form_tag(airports_url, method: 'post') do %>


  <div>
    <%= label_tag :abbrev %><br />
    <%= text_field_tag :abbrev %>
  </div>

    <div>
    <%= label_tag :city %><br />
    <%= text_field_tag :city %>
  </div>

  
  <div>
    <%= submit_tag "Create Airport" %>
  </div>
<% end %>
