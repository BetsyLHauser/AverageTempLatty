<h1>New City</h1>


<%= form_tag(cities_url, method: 'post') do %>


  <div>
    <%= label_tag :name %><br />
    <%= text_field_tag :name %>
  </div>

    <div>
    <%= label_tag :airport_id %><br />
    <%= number_field_tag :airport_id %>
  </div>

    <div>
    <%= label_tag :temp_id %><br />
    <%= number_field_tag :temp_id %>
  </div>

  
  <div>
    <%= submit_tag "Create City" %>
  </div>
<% end %>
